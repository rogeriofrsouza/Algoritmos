algoritmo "contador1a10"
var
   C: Inteiro
inicio
   Para C <- 1 ate 10 faca
        EscrevaL(C)
   FimPara
   LimpaTela
   
   Para C <- 1 ate 10 passo 2 faca
        EscrevaL(C)
   FimPara
   LimpaTela
   
   Para C <- 30 ate 1 passo -3 faca
        EscrevaL(C)
   FimPara
   LimpaTela
fimalgoritmo
