algoritmo "Somador"
var
   n1, n2: Inteiro
   
Procedimento Somador(A, B: Inteiro)
inicio
      Escreva("A soma entre ", A, " e ", B, " é = ", A + B)
FimProcedimento

inicio
      Escreva("Digite um número: ")
      Leia(n1)
      Escreva("Digite outro número: ")
      Leia(n2)
      Somador(n1, n2)
fimalgoritmo
