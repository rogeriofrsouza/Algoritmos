algoritmo "Somador"
var
   A, B, S: Inteiro
   
Funcao Somador(X, Y: Inteiro): Inteiro
inicio
       Retorne X + Y
FimFuncao

inicio
      Escreva("Digite um número: ")
      Leia(A)
      Escreva("Digite outro número: ")
      Leia(B)
      
      S <- Somador(A, B)
      Escreval("A soma entre ", A, " e ", B, " é = ", S)
fimalgoritmo
